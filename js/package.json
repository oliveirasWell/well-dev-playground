{
  "name": "well-dev-playground",
  "description": "Well Dev Playground",
  "email": "well.oliveira.snt@gmail.com",
  "version": "1.0.0",
  "engines": {
    "node": ">=10",
    "yarn": "^1.12"
  },
  "devDependencies": {
    "@babel/plugin-proposal-dynamic-import": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-member-expression-literals": "^7.8.3",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.8.3",
    "@babel/plugin-transform-property-literals": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/plugin-transform-reserved-words": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@cypress/skip-test": "^2.5.0",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-options": "^5.3.19",
    "@storybook/react": "5.3.19",
    "@tecsinapse/eslint-config-tecsinapse-js": "^3.0.3",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^9.4.0",
    "@types/material-ui": "^0.21.7",
    "babel-jest": "^24.9.0",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "cross-env": "^7.0.2",
    "cypress": "^4.9.0",
    "danger": "^9.1.5",
    "documentation": "^12.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-mdx": "^1.7.0",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "firebase-tools": "^8.4.3",
    "get-yarn-workspaces": "^1.0.2",
    "husky": "^4.2.5",
    "jest": "24.9.0",
    "jest-transform-css": "^2.0.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.11.0",
    "redux-devtools": "^3.4.2",
    "rimraf": "^2.6.3",
    "source-map-explorer": "^2.1.0",
    "start-server-and-test": "^1.10.11",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "webpack": "4.42.0"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings=0 --fix",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "index.js",
  "private": true,
  "scripts": {
    "storybook:build": "build-storybook -c .storybook -s ./storybook-public -o docs/",
    "storybook:start": "start-storybook -p 5555",
    "eslint:fix": "npx eslint packages/react-ui/src packages/shared/src --ext .js --fix",
    "analyze": "npx source-map-explorer 'build/static/js/*.js'",
    "build:docs:html": "documentation --github --config documentation.yml --babel=./babel.config.js build packages/* -f html -o docs",
    "docs:serve": "documentation serve packages/* --watch --github --config documentation.yml --babel=./babel.config.js",
    "build:docs:md": "documentation --github --config documentation.yml --babel=./babel.config.js packages/* src/* -f md > md-docs/README.md",
    "hooks": "bash .git/hooks/pre-commit",
    "lint:staged": "lint-staged",
    "test": "jest --coverage",
    "web:start": "yarn workspace well-dev-playground-react-ui start",
    "web:firebase:deploy": "yarn workspace well-dev-playground-react-ui build && firebase deploy",
    "app:android:start": "yarn workspace well-dev-playground-app start:android",
    "app:ios:start": "yarn workspace well-dev-playground-app start:ios",
    "start:server": "yarn workspace well-dev-playground-server start"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
